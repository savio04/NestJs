  "paths":{
      "/categories":{
          "post":{
              "tags": ["Doctor"],
              "summary":"Create Doctor",
              "description": "Create a new doctor",
              "requestBody":{
                  "content":{
                      "application/json":{
                          "schema":{
                              "type": "object",
                              "properties":{
                                  "name":{
                                      "type": "string"
                                  },
                                  "description":{
                                      "type":"string"
                                  }
                              },
                              "example":{
                                  "name": "doctor name",
                                  "description": "doctor description"
                              }
                          }
                      }
                  }
              },
              "responses": {
                  "201":{
                      "description": "Created"
                  },
                  "400":{
                      "description": "Already category exiting"
                  }
              }
          },
          "get":{
              "tags": ["Category"],
              "summary": "List all categories",
              "descriprion": "List all catgories",
              "responses":{
                  "200":{
                      "description": "Success",
                      "content":{
                          "application/json":{
                              "schema":{
                                  "type": "array",
                                  "items":{
                                      "type":"object",
                                      "properties":{
                                          "name": {
                                              "type":"string"
                                          },
                                          "description":{
                                              "type":"string"
                                          }
                                      }
                                  }
                              }
                          }
                      }
                  }    
              }
          }
      },
      "/categories/import":{
          "post":{
              "tags": ["Category"],
              "summary": "Upload new category",
              "description": "upload new category",
              "requestBody":{
                  "content": {
                      "multipart/form-data":{
                          "schema":{
                              "type": "object",
                              "properties": {
                                  "file":{
                                      "type": "string",
                                      "format":"binary"
                                  }
                              }
                          }
                      }
                  }
              },
              "responses":{
                  "201":{
                      "description": "Created"
                  }
              }
          }
      },
      "/specification":{
          "post":{
              "tags": ["specifications"],
              "summary": "Create a specification",
              "description": "Create a new specification",
              "requestBody":{
                  "content": {
                      "application/json":{
                          "schema":{
                              "$ref": "#/definitions/specification"
                          }
                      }
                  }
              },
              "responses":{
                  "201":{
                      "description": "Created"
                  },
                  "500":{
                      "description": "Specification already existe"
                  }
              }
          }
      }
  },
  "definitions":{
      "specification":{
          "type": "object",
          "properties":{
              "name":{
                  "type":"string"
              },
              "description":{
                  "type": "string"
              }
          }
      }
  }
}